meta {
  name: Search User by Name
  type: http
  seq: 4
}

get {
  url: {{base_url}}/users/search/Alice Johnson
}

tests {
  test("Status is 200 or 404", function() {
    expect([200, 404]).to.include(res.getStatus());
  });
  
  test("If found, has user data", function() {
    if(res.getStatus() === 200) {
      expect(res.body.id).to.exist;
      expect(res.body.name).to.equal("Alice Johnson");
    }
  });
}
